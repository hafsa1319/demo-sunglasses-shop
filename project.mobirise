        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "h3, h4, p, span": {
              "margin": "0"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".item-img, .item-box-img": {
              "amp-img, img": {
                "object-fit": "cover",
                "height": "500px",
                "@media (max-width: 992px)": {
                  "height": "300px"
                }
              },
              "align-items": "center"
            },
            ".item-box-img": {
              "padding": "0"
            },
            "@media (min-width: 1400px)": {
              ".container-fluid": {
                "padding": "0 2rem"
              }
            },
            ".item-box": {
              "background": "#efefef",
              "justify-content": "center",
              "width": "100%",
              "overflow": "auto",
              "@media (min-width: 992px)": {
                "max-width": "1000px"
              },
              "@media (max-width: 991px)": {
                "width": "90%",
                "margin": "1rem",
                "padding-top": "3rem"
              }
            },
            ".close:before, .close:after": {
              "background-color": "#777"
            },
            ".item-wrapper": {
              "cursor": "pointer",
              ".btn": {
                "padding": "1.2rem 5rem",
                "transform": "translateY(4rem)",
                "transition": "all 0.3s"
              }
            },
            ".item:focus, span:focus": {
              "outline": "none"
            },
            ".item": {
              ":hover": {
                "cursor": "pointer"
              },
              ".item-box-text, .cur, .item-box-price": {
                "&:hover": {
                  "cursor": "default"
                }
              },
              "&:hover": {
                ".btn": {
                  "transform": "translateY(0rem)"
                }
              }
            },
            ".item-wrapper, .item-box, .item-img": {
              "position": "relative"
            },
            ".item-btn, .item-box-btn": {
              "margin": "0"
            },
            ".item-box-title": {
              "color": "#000"
            },
            ".item-box-text": {
              "color": "#999999"
            },
            ".amp-shop__title": {
              "text-align": "center"
            },
            ".item-box-wrapper": {
              "background-color": "#efefef"
            },
            "& when (@customStyles)": {
              "& when (@addPadding)": {
                "& when not (@imgPadding)": {
                  ".item-wrapper": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                },
                "& when (@imgPadding)": {
                  ".item-img": {
                    "padding-bottom": "0"
                  },
                  ".item-content": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                }
              },
              "& when (@boxShadow)": {
                ".item-wrapper": {
                  "box-shadow": "0px 0px (@shadowSize * 1px) 0px rgba(red(@shadowColor), green(@shadowColor), blue(@shadowColor), @shadowOpacity)"
                }
              },
              "& when (@useColorCard)": {
                ".item-wrapper": {
                  "position": "relative",
                  "z-index": "0",
                  "&:after": {
                    "content": "\"\"",
                    "display": "block",
                    "position": "absolute",
                    "top": "0",
                    "bottom": "0",
                    "left": "0",
                    "right": "0",
                    "opacity": "@opacityCard",
                    "background": "@colorCard",
                    "transition": "opacity .3s",
                    "pointer-events": "none",
                    "z-index": "-1"
                  }
                }
              },
              "& when (@useBorderRadius)": {
                ".item-wrapper": {
                  "z-index": "12",
                  "border-radius": "(@borderRadius * 1px)",
                  "overflow": "hidden"
                }
              },
              ".item-wrapper": {
                "color": "@fontColor"
              },
              ".btn": {
                "padding": "1.2rem 5rem",
                "background-color": "transparent !important",
                "border-color": "@btnColor !important",
                "&:before, &:after": {
                  "background": "@btnColor !important"
                },
                "&:hover, &:focus, &:active": {
                  "color": "@btnColor !important",
                  "border-color": "@btnColor !important"
                }
              }
            },
            ".item-content": {
              "overflow": "hidden"
            },
            ".item-box-text1": {
              "color": "#757575"
            },
            "@media (max-width: 767px)": {
              ".item": {
                ".btn": {
                  "transform": "translateY(0rem)"
                }
              },
              "& when (@addPadding)": {
                ".item-wrapper": {
                  "padding": "(@yPadding * 1rem) 1rem"
                }
              }
            }
          },
          "_name": "shop01",
          "_customHTML": "<section class=\"amp-shop shop1\" group=\"Shop\" plugins=\"amp-main, amp-lightbox, amp-bind\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Title\" name=\"titleSection\">\n        <input type=\"checkbox\" name=\"showText\" title=\"Show Text\">\n        <header condition=\"customStyle\">Custom Styles</header>\n        <input type=\"checkbox\" name=\"customStyles\" title=\"Custom Style\" checked=\"\">\n        <header condition=\"customStyles\">Card Color</header>\n        <input type=\"checkbox\" name=\"useColorCard\" title=\"Use Background Card\" condition=\"customStyles\" checked=\"\">\n        <input type=\"color\" name=\"colorCard\" title=\"Color\" value=\"#f5f5f5\" condition=\"useColorCard &amp;&amp; customStyles\">\n        <input type=\"range\" title=\"Opacity\" name=\"opacityCard\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"useColorCard &amp;&amp; customStyles\">\n        <input type=\"color\" name=\"fontColor\" title=\"Font Color\" value=\"#4a4a4a\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnColor\" title=\"Btn Color\" value=\"#232323\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnText\" title=\"Btn Text Color\" value=\"#ffffff\" condition=\"customStyles\">\n\n        <header condition=\"customStyles\">Paddings</header>\n        <input type=\"checkbox\" name=\"addPadding\" title=\"Add Padding\" condition=\"customStyles\" checked=\"\">\n        <input type=\"checkbox\" name=\"imgPadding\" title=\"No Image Padding\" condition=\"customStyles &amp;&amp; addPadding\">\n    