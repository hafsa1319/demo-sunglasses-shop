          "_name": "footer02",
          "_customHTML": "<section class=\"form footer2\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\" global=\"\" once=\"footer\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Form Text\" name=\"showText\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Col 1 Items\" name=\"items\" min=\"1\" max=\"5\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Col 2 Items\" name=\"items1\" min=\"1\" max=\"5\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Col 3 Items\" name=\"items2\" min=\"1\" max=\"5\" step=\"1\" value=\"5\">\n        <header>Style</header>\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\" checked=\"\">\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\">\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusText\" title=\"Text Focus\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusPlaceholder\" title=\"Placeholders Focus\" value=\"#000000\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row\">\n            <div class=\"mbr-col-lg-2 mbr-col-md-4 mbr-col-sm-12 md-pb\">\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-2 align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showTitle\"><b>Shop</b></h3>\n                <div class=\"item\" mbr-if=\"items>0\">\n\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">New Arrivals</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items>1\">\n\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Best Sellers</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items>2\">\n\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Gift Guide</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items>3\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Sales</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items>4\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Visit Us</p>\n                </div>\n            </div>\n            <div class=\"mbr-col-lg-2 mbr-col-md-4 mbr-col-sm-12 md-pb\">\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-2 align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showTitle\"><b>Explore</b></h3>\n                <div class=\"item\" mbr-if=\"items1>0\">\n\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">About Us</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items1>1\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Collaborations</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items1>2\">\n\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Product Care</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items1>3\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Stories</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items1>4\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Lookbooks</p>\n                </div>\n            </div>\n            <div class=\"mbr-col-lg-2 mbr-col-md-4 mbr-col-sm-12 md-pb\">\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-2 align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showTitle\"><b>More</b></h3>\n                <div class=\"item\" mbr-if=\"items2>0\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">My Account</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items2>1\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Custom Orders</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items2>2\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Size Guide</p>\n                </div>\n                <div class=\"item\" mbr-if=\"items2>3\">\n\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Careers</p>\n                </div>\n\n                <div class=\"item\" mbr-if=\"items2>4\">\n                    <p class=\"mbr-fonts-style mbr-text align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Contact</p>\n                </div>\n            </div>\n            <div class=\"mbr-col-lg-3 mbr-col-md-12 mbr-col-sm-12\">\n            </div>\n            <div mbr-form=\"\" class=\"mbr-col-lg-3 mbr-col-md-12 mbr-col-sm-12 mbr-form\">\n                <div class=\"wrapper\">\n                    <h3 class=\"mbr-section-title mbr-black mbr-fonts-style mbr-pb-2 align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\">Exclusive\n                        benefits